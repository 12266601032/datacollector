<!--
  Common assembly used by stage library modules
-->
<assembly>
  <id>stage-lib</id>
  <formats>
    <format>dir</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>

  <fileSets>
  </fileSets>

  <dependencySets>
    <dependencySet>
      <includes>
        <include>org.python:jython-standalone</include>
      </includes>
      <outputFileNameMapping>jython.jar</outputFileNameMapping>
      <outputDirectory>/${project.artifactId}/lib</outputDirectory>
    </dependencySet>
  </dependencySets>

</assembly>

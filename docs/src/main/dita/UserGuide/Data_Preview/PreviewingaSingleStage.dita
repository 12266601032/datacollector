                <cmd/>
            </step>
            <step
                conref="../Reusable_Content/ReusableSteps.dita#task_kzs_5vz_sq/DeletePreviewRecord">
                <cmd/>
            </step>
            <step conref="../Reusable_Content/ReusableSteps.dita#task_kzs_5vz_sq/NextStage">
                <cmd/>
            </step>
            <step conref="../Reusable_Content/ReusableSteps.dita#task_kzs_5vz_sq/EditingPData1"
                conrefend="../Reusable_Content/ReusableSteps.dita#task_kzs_5vz_sq/EditingPData2">
                <cmd/>
            </step>
            <step>
                <cmd>To view the changed data in other stages, use the stage navigation icons or
                    select the stage to review. </cmd>
            </step>
